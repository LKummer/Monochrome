{
  "name": "hugo-theme-boilerplate",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "run-s build:theme build:demo",
    "build:demo": "hugo -s demo -d ../dist/demo --themesDir ../dist",
    "build:theme": "webpack",
    "build:theme:prod": "webpack --env.prod",
    "build:storybook": "build-storybook",
    "dev": "run-s build:theme dev:start",
    "dev:start": "run-p dev:theme dev:demo",
    "dev:demo": "hugo serve -s demo -d ../dist/demo --themesDir ../dist -vw",
    "dev:theme": "webpack -w"
  },
