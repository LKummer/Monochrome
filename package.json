    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^6.0.0",
    "hugo-bin": "^0.52.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
